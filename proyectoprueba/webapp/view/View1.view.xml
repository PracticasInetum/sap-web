<mvc:View controllerName="proyectoprueba.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
            <List
                headerText="Products"
                headerLevel="H2"
                items="{
                    path: 'Productos>/ListaProductos'
                }" >
                <StandardListItem
                    type="Navigation"
                    press="onListItemPress"
                    title="{Productos>Nombre}"
                    counter="{Productos>Precio}"/>
	        </List>
        </content>
    </Page>
</mvc:View>
